
vanguard begin() {
    hashtable := createHashTable@heap(1)

    hashtable.putValue@heap("Leo.Englert", "Leo.Englert@outlook.de")
    hashtable.putValue@heap("Leo :D", "Leo.Englert@outlook.de")

    for true {
        email := hashtable.getValue@heap(input("> "))
        if email.Length() > 0
            output(email)
        else
            output("Couldn't find entry")
    }

    hashtable.destroyHashTable@heap()
}